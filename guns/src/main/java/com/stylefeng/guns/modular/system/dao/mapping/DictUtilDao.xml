<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.DicUtilDao">

	<select id="getTeacherName" resultType="map">
		select id,name from user
		where
		status=1 and roleid=7
	</select>
	
	<select id="getMeterialName" resultType="map">
		select id,name from material
	</select>
	
	
</mapper>

